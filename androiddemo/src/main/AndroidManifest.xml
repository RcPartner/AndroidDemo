<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rc.androiddemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"></category>
                <action android:name="android.intent.action.MAIN"></action>
            </intent-filter>
        </activity>

        <!--ui-->
        <!--TextView-->
        <activity android:name=".ui.textview.TextViewDemo" />
        <!--EditTextDemo-->
        <activity android:name=".ui.edittext.EditTextDemo" />
        <!--动画-->
        <activity android:name=".ui.animation.TweenedAnimation" />
        <activity android:name=".ui.animation.view.tween.AlphaDemo" />
        <activity android:name=".ui.animation.view.tween.RotateDemo" />
        <activity android:name=".ui.animation.view.tween.ScaleDemo" />
        <activity android:name=".ui.animation.view.tween.TranslateDemo" />
        <!--评分条-->
        <activity android:name=".ui.ratingbar.RatingBarDemo" />
        <!--webview-->
        <activity android:name=".ui.webview.WebViewDemo" />
        <!--开关按钮-->
        <activity android:name=".ui.switchh.SwitchDemo" />
        <!--浮窗-->
        <activity android:name=".ui.dialog.AlertDialogDemo" />

        <activity android:name=".ui.custom.TwoScrollViewDemo" />


        <!--Bitmap-->
        <activity android:name=".resources.drawable.bitmapfile.BitmapFileDemo" />
        <!--层叠-->
        <activity android:name=".resources.drawable.layerlist.LayerListDemo" />
        <!--.9-->
        <activity android:name=".resources.drawable.ninepatchfile.NinePatchFileDemo" />
        <!-- 触摸事件-->
        <activity android:name=".ui.touchevent.TouchEventDemo" />
        <!-- MVP模式-->
        <activity android:name=".design.mvpdemo.view.MVPLoginDemo" />
        <!-- 动画-->
        <activity android:name=".ui.animation.view.tween.AnimationDemo" />
        <!-- dialogfragment弹窗-->
        <activity android:name=".ui.dialog.DialogFragmentDemo" />
        <!-- -->

        <activity android:name=".ui.ViewGroupEventDemo" />

